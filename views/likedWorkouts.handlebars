<h2>Your Liked Workouts</h2>

{{#each workouts as |workout|}}
<div>
    <div>
      <h2>
        <a href="/workouts/{{id}}">{{title}}</a>
      </h2>
      <h4>{{category.name}}</h4>
      <p>
        {{description}}
      </p>
      <p>Added by:
        <a href="/profile/{{user.id}}">{{user.name}}</a>
        on
        {{format_date createdAt}}</p>
    </div>
  </div>
{{/each}}